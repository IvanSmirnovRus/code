    "from bottle import run, route"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "@route('/')\n",
    "def main():\n",
    "    result = \"\"\n",
    "    with open('index.html', 'r') as main_page:\n",
    "        for line in main_page:\n",
    "            result += line\n",
    "    return result"
   ]
